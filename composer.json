{
    "name": "services/slim4-starter-template",
    "type": "project",
    "description": "Slim4 starter template",
    "keywords": [],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "guzzlehttp/psr7": "^2",
        "laminas/laminas-diactoros": "^2.20",
        "nyholm/psr7": "^1.5",
        "nyholm/psr7-server": "^1.0",
        "respect/validation": "^2.2",
        "slim/psr7": "^1.5",
        "slim/slim": "4.*",
        "php-di/slim-bridge": "^3.2",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.12",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "phpunit": "phpunit",
        "fix-cs": "php-cs-fixer fix",
        "test-cs": "php-cs-fixer fix --dry-run -v",
        "test": ["@phpunit", "@test-cs"]
    }
}
